{
  "name": "oc-component-ui-rb",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/oc-component-ui-rb.cjs.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/oc-component-ui-rb.es.js",
  "exports": {
    "./forms": {
      "import": "./dist/forms/oc-component-ui-rb.es.js",
      "require": "./dist/forms/oc-component-ui-rb.cjs.js",
      "types": "./dist/forms/index.d.ts"
    },
    "./nav": {
      "import": "./dist/nav/oc-component-ui-rb.es.js",
      "require": "./dist/nav/oc-component-ui-rb.cjs.js",
      "types": "./dist/nav/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/oc-component-ui-rb.es.js",
      "require": "./dist/common/oc-component-ui-rb.cjs.js",
      "types": "./dist/common/index.d.ts"
    },
    ".": {
      "import": "./dist/oc-component-ui-rb.es.js",
      "require": "./dist/oc-component-ui-rb.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1"
  }
}
